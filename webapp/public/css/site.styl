@import url('https://fonts.googleapis.com/css?family=Roboto:400,700');

body
  padding-top 69px
  background-color #F7F7F7
  font-family: 'Roboto', sans-serif;
.content
  margin-top 50px
div.navbar-fixed-top
  background-color #073053
  color #ffffff
  padding 10px 0
  margin-bottom 50px
.navbar-header
  margin 0 10px 0 10px
a.navbar-link
  color #ffffff
  background-color: #073053
  transition: all 0.2s
  margin 0 5px
ul.nav>li>a.navbar-link:hover, ul.nav>li>a.navbar-link:active, ul.nav>li>a.navbar-link:focus
  box-shadow: inset 0 -5px 6px -5px #ffffff
  background-color #073053
.navbar-signin
  float right!important
a.navbar-btn-signin
  color #ffffff
  background-color #C65627
a.navbar-btn-signin:hover
  color #ffffff
  background-color #aa4b22
a.navbar-btn
  margin 0 0 0 10px
a.navbar-profile
  color #ffffff
  background-color: #073053
ul.nav>li>a.navbar-profile:hover, ul.nav>li>a.navbar-profile:active, ul.nav>li>a.navbar-profile:focus
  color #C65627
  background-color #073053
footer
  margin-top 50px
  padding 20px 0
  border-top 1px solid #d7e0e2

.navbar-form
  .form-group
    margin-left 10px
  button
    margin-left 10px

.top-padding-med
  padding-top 10px

.top-padding-lg
  padding-top 25px

.bottom-padding-lg
  padding-bottom 25px

.margin-left-med
  margin-left 10px

.toast-top-right
  top 70px

.toast-success
  background-color #13987e

.container .jumbotron
  margin-top 100px

.home-button
  width: 100%
  margin-bottom: 10px

.crop-model-box
  border 2px
  width: 200px
  height: 200px
.model-image-box
  border 2px
  width: 50px
  height: 50px
.model-image-box-100
  border 2px
  width: 100px
  height: 100px
.wireframe-crop-image-box
  position: absolute
  top: -999999px
.control-marker-box
  position: absolute
  background-color: #42f4d4
  border: 1px solid black
  opacity: 0.6
.no-padding-box
  padding : 0px
.public-extra-margin
  margin-top : 100px
div.preview-canvas-wrapper
  width 95%
  max-width 95%
.preview-canvas
  border: 1px solid #cccccc
  width: 1000px
  height: 1000px
  margin 2% auto
  box-shadow 10px 10px 5px #cccccc
  background-color #ffffff
  position relative
  overflow scroll
thead
  background-color #e6e6e6
  font-weight 500
table
  border 1px solid #e6e6e6
tbody.tr
  padding-top 100px
a.tile
  text-decoration none
  background-color #ffffff
  box-shadow 0 1px 3px 0 rgba(0,0,0,.2)
  border-radius 10px
  padding 10px 15px
  margin 8px 10px
  transition-property transform
  transition-duration 0.25s
  transition-timing-function ease
a.tile:hover
  cursor: pointer
  -ms-transform scale(1.05, 1.05)
  -webkit-transform scale(1.05, 1.05)
  transform scale(1.05, 1.05)
div.tile-row
  margin 0 0
img.tile-img
  max-width 100%
  height 100px
  margin 15px 80px 5px 80px
p.tile-text
  color #333333
h2.tile-title
  color #f06b50

.proto-div
  border-bottom solid 2px black