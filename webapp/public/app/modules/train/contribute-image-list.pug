.container(ng-hide="identity.isAuthenticated()")
    .jumbotron
        h1 Wireframe Analysis Engine
        p Please Log IN to view Wireframe Analysis Engine Reports.
.container.top-padding-med(ng-show="identity.isAuthenticated()")
    .pull-left
        a.btn.btn-primary(href='/contribute/upload') Upload
    .pull-right
        form.form-inline
            .form-group
                input.form-control(ng-model="searchText", placeholder="Filter")
            .form-group.margin-left-med
                select.form-control(ng-model="sortOrder", ng-options="item.value as item.text for item in sortOptions")
    table.table.table-hover.table-striped.table-condensed
        thead
            tr
                th Control
                th Upload Date
                th Image Type
                th Status
                th Actual Lavel
                th Actual Text
                th Predicted Lable
                th Predicted Text
        tbody
            tr(ng-repeat="data in userContributeData | filter:searchText | orderBy:sortOrder")
                td
                    img.model-image-box(ng-src='/api/page/image/{{data._id}}')
                td {{data.uploaded_on | date}}
                td {{data.image_type}}
                td {{data.status}}
                td {{data.actual_label}}
                td {{data.actual_text}}
                td(ng-if='data.prediction_label.length === 0')
                |   &nbsp;
                //td(ng-if='data.prediction_label.length > 0', ng-repeat="item in data.prediction_label")
                //|   {{item.provider}} -- {{item.result}}
                td(ng-if='data.prediction_text.length === 0')
                |   &nbsp;
                //td(ng-if='data.prediction_label.length > 0', ng-repeat="item in data.prediction_text")
                //|   {{item.provider}} -- {{item.result}}
                td
                    button.btn.btn-primary(ng-click="deleteControl(data._id)") X